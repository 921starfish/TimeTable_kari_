<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TimeTableOne.Common">
    <Color x:Key="OneNotePurpleColor" >#FF80397B</Color>
    <Color x:Key="EditBoxGrayColor">#FFE8E2E8</Color>
    <SolidColorBrush x:Key="OneNotePurple" Color="{StaticResource OneNotePurpleColor}"/>
    <SolidColorBrush x:Key="SelectedOneNotePurple" Color="#FFA3569E"/>
    <SolidColorBrush x:Key="EditBoxGray" Color="{StaticResource EditBoxGrayColor}"/>
    <SolidColorBrush x:Key="TextBoxGray" Color="#FFF0EDF3"/>
    <SolidColorBrush x:Key="TextBoxBorder" Color="#FFE0DADE"/>
    <SolidColorBrush x:Key="DetailBoxBlack" Color="#FF212121"/>
    <SolidColorBrush x:Key="SelectedDetailBoxBlack" Color="#FF474747"/>
</ResourceDictionary>
